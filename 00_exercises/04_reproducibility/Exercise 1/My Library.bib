@article{skrondal2000,
  title = {Design and {{Analysis}} of {{Monte Carlo Experiments}}: {{Attacking}} the {{Conventional Wisdom}}},
  shorttitle = {Design and {{Analysis}} of {{Monte Carlo Experiments}}},
  author = {Skrondal, Anders},
  year = 2000,
